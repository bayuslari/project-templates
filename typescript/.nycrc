{
    "reporter": [
        "lcov",
        "text",
        "html"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
        "ts-node/register"
    ],
    "temp-dir": "./tmp/.nyc_output",
    "report-dir": "./tmp/.test-coverage",
    "sourceMap": true,
    "instrument": true,
    "all": true,
     "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "cache": true,
    "check-coverage": true
}