{
  "name": "typescript-project-template",
  "version": "1.0.0",
  "description": "template for typescript node project",
  "main": "dist/src",
  "scripts": {
    "start": "NODE_PATH=./dist NODE_ENV=production node .",
    "build": "rm -fr ./dist/* && tsc",
    "build:dev": "tsc --sourceMap --removeComments false --pretty",
    "dev": "NODE_PATH=./dist nodemon --verbose",
    "mocha": "mocha",
    "test": "nyc mocha -- test/**/*.test.ts",
    "lint": "eslint src/. tests/."
  },
  "engines": {
    "node": "^12.0.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.5",
    "@types/sinon": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "winston": "^3.2.1"
  }
}